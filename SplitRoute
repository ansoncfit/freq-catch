   "cell_type": "code",
   "execution_count": 109,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Splits each route into trip-specific routes.\n",
    "\n",
    "import partridge as pa\n",
    "import pandas as pd\n",
    "import sys\n",
    "from copy import deepcopy\n",
    "\n",
    "feedFile = 'data/' + 'lossan' + '/amtrak.zip'\n",
    "\n",
    "feed = pa.load_feed(feedFile)\n",
    "\n",
    "r = feed.routes\n",
    "\n",
    "t = feed.trips\n",
    "t.set_index('trip_id', inplace = True)\n",
    "t\n",
    "\n",
    "c = feed.calendar"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 106,
   "metadata": {},
   "outputs": [],
   "source": [
    "newRouteIds = []\n",
    "newRoutes = pd.DataFrame(columns = r.columns)\n",
    "newTrips = pd.DataFrame(columns = t.columns)\n",
    "newTrips.index.name = 'trip_id'\n",
    "\n",
    "for trip_id, trip in t.iterrows():\n",
    "    newTrip = deepcopy(trip)\n",
    "    newRouteId = str(trip.route_id) + '-' + str(trip.trip_short_name)\n",
    "    \n",
    "    newTrip.route_id = newRouteId\n",
    "    newTrips = newTrips.append(newTrip)\n",
    "    \n",
    "    if newRouteId not in newRouteIds:\n",
    "    \n",
    "        newRoute = deepcopy(r.loc[r['route_id'] == trip.route_id])\n",
    "        newRoute.route_id = newRouteId\n",
    "        newRoute.route_short_name = trip.trip_short_name\n",
    "        newRoute.route_long_name += ' - ' + trip.trip_headsign\n",
    "        newRoutes = newRoutes.append(newRoute, ignore_index = True)\n",
    "        newRouteIds.append(newRouteId)\n",
    "\n",
    "newRoutes.set_index('route_id').to_csv('data/lossan/modified/routes.txt')\n",
    "newTrips.to_csv('data/lossan/modified/trips.txt')"
   ]
  },
